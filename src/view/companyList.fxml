<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@css/style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CompanyMenuController">
    <children>
        <TableView fx:id="tableView" prefHeight="690.0" prefWidth="980.0">
            <columns>
                <TableColumn fx:id="name" prefWidth="93.0" text="Name" />
                <TableColumn fx:id="mail" minWidth="5.0" prefWidth="123.0" text="Mail" />
                <TableColumn fx:id="ICO" prefWidth="160.0" text="ICO" />
                <TableColumn fx:id="phone" prefWidth="159.0" text="Phone" />
                <TableColumn fx:id="inspectionName" prefWidth="152.0" text="Inspection" />
            </columns>
        </TableView>
      <AnchorPane fx:id="tableButtonPane" layoutX="568.0" layoutY="570.0" prefHeight="100.0" prefWidth="382.0">
         <children>
            <JFXButton fx:id="expandButton" layoutX="12.0" layoutY="18.0" onAction="#expand" prefHeight="65.0" prefWidth="177.0" styleClass="pinkButton" text="Expand" textFill="WHITE" />
            <JFXButton fx:id="removeCompanyButton" layoutX="192.0" layoutY="18.0" onAction="#removeCompany" prefHeight="65.0" prefWidth="177.0" styleClass="pinkButton" text="Remove Company" textFill="WHITE" />
         </children>
      </AnchorPane>
    </children>
</AnchorPane>
